  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ReporteInventario</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Store">
            <Value>=Parameters!Store.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Category">
            <Value>=Parameters!Category.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Brand">
            <Value>=Parameters!Brand.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
    s.store_name AS StoreName,
    c.category_name AS CategoryName,
    b.brand_name AS BrandName,
    p.product_name AS ProductName,
    SUM(st.quantity) AS TotalInventory
FROM production.stocks st
JOIN production.products p ON st.product_id = p.product_id
JOIN production.categories c ON p.category_id = c.category_id
JOIN production.brands b ON p.brand_id = b.brand_id
JOIN sales.stores s ON st.store_id = s.store_id
WHERE
    (@Store IS NULL OR @Store = '' OR s.store_name = @Store)
    AND (@Category IS NULL OR @Category = '' OR c.category_name = @Category)
    AND (@Brand IS NULL OR @Brand = '' OR b.brand_name = @Brand)
GROUP BY
    s.store_name,
    c.category_name,
    b.brand_name,
    p.product_name
ORDER BY
    s.store_name,
    c.category_name,
    b.brand_name,
    p.product_name;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
