          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DS_StoreList">
      <Query>
        <DataSourceName>ReporteInventario</DataSourceName>
        <CommandText>SELECT NULL AS StoreName
UNION
SELECT DISTINCT store_name
FROM sales.stores
ORDER BY StoreName;
</CommandText>
      </Query>
